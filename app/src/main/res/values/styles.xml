<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.NoActionBar" />
    <style name="AppTheme" parent="AppBaseTheme">
    </style>
    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>
    <style name="CustomProgressDialog" parent="@style/CustomDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- <item name="android:windowNoTitle">true</item> -->
    </style>
    <style name="style_common_btn">
        <item name="android:background">@drawable/selector_btn_btn_txt_login</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/style_common_btn_padding</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">@dimen/new_button_long_text</item>
    </style>
    <!-- 按钮样式 -->
    <style name="set_Button_styles">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimens_rl_height</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/dimens_button</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_margin">@dimen/dimens_rl_layout_margin</item>
    </style>
    <!-- 按钮样式 -->
    <style name="style_register_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/register_tv_text_size</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:textColor">#000</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:minWidth">86dp</item>
    </style>
    <style name="style_common_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="style_range_text1">
